<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sz_device">

    <application
        android:name=".AppInit"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <!--<category android:name="android.intent.category.DEFAULT" />-->
                <!--<category android:name="android.intent.category.HOME" />-->
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPersonActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".IndexActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".New_IndexActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".WYYActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".WYYAddPersonActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".WYYAddActivity2" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_ZheJiang.ZJYZBAddActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_ZheJiang.ZJYZBActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".HNMBYActivity" android:theme="@style/ImageTranslucentTheme"></activity>

        <activity android:name=".Activity_YZBYUN.IndexActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_YZBYUN.MC.MCIndexActivity" android:theme="@style/ImageTranslucentTheme"></activity>

        <activity android:name=".StartActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_SX.MainActivity4" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_SX.AddPersonActivity2" android:theme="@style/ImageTranslucentTheme"></activity>

        <activity android:name=".Activity_GDYZB.MainActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_GDYZB.AddPersonActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <service android:name=".Activity_GDYZB.SwitchService"></service>

        <activity android:name=".Activity_HEBEI.IndexActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_HEBEI.AddPersonActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <service android:name=".Activity_HEBEI.HebeiSwitchService"></service>

        <activity android:name=".Activity_ShaoXing.ShaoXingActivity" android:theme="@style/ImageTranslucentTheme"></activity>
        <activity android:name=".Activity_ShaoXing.ShaoXingAddActivity" android:theme="@style/ImageTranslucentTheme"></activity>

        <service android:name=".Activity_ShaoXing.ShaoXingService"></service>
        <service android:name=".Activity_SX.SXService"></service>
        <service android:name=".Service.HNMBYService"></service>
        <service android:name=".Service.SwitchService"></service>
        <service android:name=".Service.WYYService"></service>
        <service android:name=".Activity_YZBYUN.YunService"></service>
        <service android:name=".Activity_ZheJiang.ZJYZBService"></service>
        <service android:name=".Activity_YZBYUN.MC.MCService"></service>
        <receiver android:name=".Receiver.BootBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 这个权限用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <!-- 这个权限用于访问GPS定位 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!-- 用于访问wiFi网络信息，wiFi信息会用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 这个权限用于获取wiFi的获取权限，wiFi信息会用来进行网络定位 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <!-- 用于读取手机当前的状态 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <!-- 开机启动 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"></uses-permission>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
</manifest>